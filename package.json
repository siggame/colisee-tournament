    "url": "https://github.com/siggame/colisee-tournament/issues"
  },
  "homepage": "https://github.com/siggame/colisee-tournament#readme",
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/cors": "^2.8.0",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/knex": "0.0.40",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "@types/winston": "^2.2.0",
    "apidoc": "^0.17.5",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
