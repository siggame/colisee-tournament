{
  "name": "colisee-tournament",
  "version": "1.0.0",
  "description": "Micro-service responsible for scheduling games for SIG-Game Colisee tournaments",
  "main": "src/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "find src test -name *.js -exec rm {} +",
    "docs": "apidoc --input src/ --output docs/",
    "start": "npm run quick-start",
    "setup": "npm install",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "quick-start": "node src/main.js",
    "quick-test": "mocha --colors test/",
    "test": "npm run quick-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/colisee-tournament.git"
  },
